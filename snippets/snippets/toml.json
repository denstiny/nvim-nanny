{
  "date": {
    "prefix": "date",
    "body": [
      "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}  ${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}"
    ],
    "description": "ISO date time stamp"
  },
  "tsinghua": {
    "prefix": "poetry.source.tsinghua",
    "body": [
      "[[tool.poetry.source]]",
      "name = \"tsinghua\"",
      "url = \"https://pypi.tuna.tsinghua.edu.cn/simple\"",
      "priority = \"primary\""
    ],
    "description": "添加清华源"
  },
  "pyright": {
    "prefix": "pyright",
    "body": [
      "[tool.pyright]",
      "venvPath = \"${1:.}\"",
      "venv = \"${2:.venv}\""
    ]
  }
}
